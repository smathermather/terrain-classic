{
  "/flat": {
    "source": "mapnik://./terrain-classic.xml"
  },
  "/labels": {
    "source": "mapnik://./terrain-classic-labels.xml"
  },
  "/": {
    "source": {
      "protocol": "blend:",
      "query": {
        "layers": [
          {
            "source": "mapnik://./terrain-classic.xml"
          },
          {
            "source": "http://ec2-54-152-68-8.compute-1.amazonaws.com/positron-terrain/{z}/{x}/{y}.png",
            "comp-op": "hard-light",
            "opacity": "0.3"
          },
          {
            "source": "mapnik://./terrain-classic-labels.xml"
          }
        ],
        "format": "png8"
      }
    }
  }
}
